{
  "name": "exporoutertv",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "EXPO_TV=1 expo start",
    "android": "EXPO_TV=1 expo run:android",
    "ios": "EXPO_TV=1 expo run:ios",
    "web": "expo start --web",
    "reset-project": "./scripts/reset-project.js",
    "lint": "expo lint",
    "prebuild": "expo prebuild --clean",
    "prebuild:tv": "EXPO_TV=1 expo prebuild --clean",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy"
  },
  "dependencies": {
    "@expo/cli": "^0.20.5",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "d3-shape": "^3.2.0",
    "expo": "~53.0.23",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "npm:react-native-tvos@0.79.0-0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-svg-charts": "^5.4.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@react-native-tvos/config-tv": "^0.1.4",
    "@types/d3-shape": "^3.1.7",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "private": true
}
